{
  "name": "get-reply-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:next\" \"npm run dev:inngest\"",
    "dev:next": "next dev",
    "dev:inngest": "npx inngest-cli@latest dev --port 8288",
    "dev:download-types": "supabase gen types typescript --project-id 'dwtrraqsrjkkheqloopn' --schema public > src/db-admin/database.types.ts",
    "dev:pull-secrets": "vercel env pull .env.vercel.local",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "wrangler:dev": "wrangler dev",
    "wrangler:deploy": "wrangler deploy",
    "wrangler:logs": "wrangler tail email-forwarding",
    "wrangler:target-pc": "echo 'https://pc.azorin.studio/api/process-email' | wrangler secret put GETREPLY_URL",
    "wrangler:target-laptop": "echo 'https://laptop.azorin.studio/api/process-email' | wrangler secret put GETREPLY_URL",
    "wrangler:target-prod": "echo 'https://getreply.app/api/process-email' | wrangler secret put GETREPLY_URL"
  },
  "dependencies": {
    "@google-cloud/local-auth": "^2.1.0",
    "@microsoft/microsoft-graph-client": "^3.0.5",
    "@supabase/auth-helpers-nextjs": "^0.7.1",
    "@supabase/auth-ui-react": "^0.4.2",
    "@supabase/auth-ui-shared": "^0.1.6",
    "@types/node": "18.15.10",
    "@types/react": "18.0.29",
    "@types/react-dom": "18.0.11",
    "autocannon": "^7.11.0",
    "eslint": "8.36.0",
    "eslint-config-next": "13.2.4",
    "googleapis": "^105.0.0",
    "jest-fetch-mock": "^3.0.3",
    "next": "^13.4.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.0.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20230511.0",
    "@radix-ui/react-icons": "1.3.0",
    "@radix-ui/react-popover": "1.0.5",
    "@supabase/auth-helpers-react": "^0.3.1",
    "@supabase/supabase-js": "^2.24.0",
    "@tailwindcss/typography": "^0.5.9",
    "@testing-library/dom": "^9.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/ms": "^0.7.31",
    "@vercel/analytics": "^1.0.0",
    "autoprefixer": "^10.4.14",
    "buffer": "6.0.3",
    "classnames": "2.3.2",
    "concurrently": "8.1.0",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "encoding": "^0.1.13",
    "inngest": "1.9.1",
    "inngest-cli": "0.14.4",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "ms": "^2.1.3",
    "nock": "^13.3.0",
    "node-email-reply-parser": "0.1.4",
    "node-fetch": "2.6.11",
    "openai": "^3.2.1",
    "postal-mime": "1.0.16",
    "react-icons": "4.9.0",
    "server-only": "^0.0.1",
    "slugify": "^1.6.6",
    "supabase": "^1.50.13",
    "tailwindcss": "^3.2.7",
    "usehooks-ts": "2.9.1",
    "vercel": "29.0.0",
